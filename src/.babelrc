{
  "env": {
    "web": {
      "presets": [
        ["env", {
          "useBuiltIns": true,
          "debug": false,
          "targets": {
            "browsers": ["last 2 versions", "safari >= 7"]
          }
        }],
        "react",
      ],
      "plugins": ["transform-class-properties"]
    },
    "server": {
      "presets": [
        ["env", {
          "useBuiltIns": true,
          "debug": false,
          "targets": {
            "node": 6.9
          }
        }],
      ],
    }
  }
}
