[ignore]
.*/__tests__/.*
.*/react/node_modules/.*
.*/fbjs/node_modules/.*
.*/node_modules/invariant/.*

[include]
./node_modules/react
./node_modules/fbjs/lib

[libs]
./node_modules/fbjs/flow/lib
./leaflet-flowtype/flow

[options]
module.system=haste
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
