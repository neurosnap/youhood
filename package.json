{
  "name": "youhood",
  "version": "1.0.0",
  "description": "Crowd sourced neighborhood map",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "github.com:neurosnap/youhood.git"
  },
  "scripts": {
    "test": "npm run circular && npm run lint && flow",
    "web": "BABEL_ENV=web NODE_ENV=production browserify src/delivery/web/index.js -o public/bundle.js -t babelify -t envify",
    "dev": "BABEL_ENV=web NODE_ENV=development watchify src/delivery/web/index.js -o public/bundle.js -t babelify -t envify",
    "server": "BABEL_ENV=server NODE_ENV=production babel src/delivery/server.js -o server.js --source-maps",
    "dev-server": "BABEL_ENV=server NODE_ENV=development babel src/delivery/server.js -o server.js -w --source-maps",
    "lint": "tslint ./src",
    "start": "xdg-open public/index.html",
    "start-server": "node ./server.js",
    "circular": "madge --circular src/delivery/web/index.js"
  },
  "author": "Eric Bower",
  "license": "MIT",
  "dependencies": {
    "@mapbox/leaflet-pip": "^1.1.0",
    "@types/express": "^4.0.36",
    "@types/leaflet": "^1.0.67",
    "@types/node": "^8.0.19",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^15.5.2",
    "@types/react-redux": "^4.4.47",
    "@types/redux-logger": "^3.0.0",
    "@types/uuid": "^3.4.0",
    "@types/websocket": "^0.0.34",
    "@types/ws": "^3.0.2",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "envify": "^4.0.0",
    "express": "^4.15.3",
    "leaflet": "^1.0.3",
    "leaflet-draw": "^0.4.9",
    "madge": "^2.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hyperscript": "^3.0.0",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.4",
    "tslint": "^5.5.0",
    "tslint-config-airbnb": "^5.2.1",
    "typescript": "^2.4.2",
    "uuid": "^3.0.1",
    "watchify": "^3.9.0",
    "ws": "^3.0.0"
  }
}
