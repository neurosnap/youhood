{
  "name": "youhood",
  "version": "1.0.0",
  "description": "Crowd sourced neighborhood map",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "github.com:neurosnap/youhood.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "web": "BABEL_ENV=web NODE_ENV=production browserify src/web.js -o public/bundle.js -t babelify",
    "dev": "BABEL_ENV=web NODE_ENV=development watchify src/web.js -o public/bundle.js -t babelify",
    "server": "BABEL_ENV=server NODE_ENV=production babel src/server.js -o server.js --source-maps ",
    "dev-server": "BABEL_ENV=server NODE_ENV=development babel src/server.js -o server.js -w --source-maps ",
    "lint": "eslint ./src --fix",
    "start": "xdg-open public/index.html",
    "start-server": "node ./server.js"
  },
  "author": "Eric Bower",
  "license": "MIT",
  "dependencies": {
    "@mapbox/leaflet-pip": "^1.1.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "express": "^4.15.3",
    "leaflet": "^1.0.3",
    "leaflet-draw": "^0.4.9",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hyperscript": "^3.0.0",
    "uuid": "^3.0.1",
    "watchify": "^3.9.0",
    "ws": "^3.0.0"
  }
}
